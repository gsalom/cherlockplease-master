<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <%- include("partials/head") %>
</head>
<body>
    <!-- HEADER -->
    <%- include("partials/navigationbackend") %>
    <!-- My Content -->
        <main class="container pt-2 text-center justify-content-center align-items-center vh-100" style="auto">
        <section>
          <h2 class="display-1">Departaments</h2><%- include("partials/paginacio") %>
          <table class="table table-bordered  fons" width="100%" cellspacing="0">
            <tr>
              <th>Codi</th>
              <th>Nom</th>
              <th>Email</th>
              <th>Import</th>
            </tr>
            <% data.forEach(function(dat){ %>
            <tr>
              <td><%= dat.codi %></td>
              <td><%= dat.nom %></td>
              <td><%= dat.email %></td>
              <td><%= dat.importGestIB %></td>
              <%  }); %>
            </tr>
          </table>
        </section>
      </main>
</body>
</html>