<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
    <%- include("partials/head") %>
    <script src="funcions.js"></script>
</head>
<body onfocus="location.reload()">
    <!-- HEADER -->
    <%- include("partials/navigation") %>
    <!-- My Content -->
        <main class="container pt-2 text-center justify-content-center align-items-center vh-100" style="auto">
        <section>
          <h1 class="display-1">Assignacions d'ordinadors: <%= carreto %> Estock: <%= numreg %></h1>
          <table class="table table-bordered fons" width="100%" cellspacing="0">
            <tr>
              <th>Posici√≥</th>
              <th>Ordinador</th>
              <th>Alumne/a</th>
              <th>Estat</th>
              <th>Comentaris</th>
            </tr>
            <% data.forEach(function(dat){ %>
            <tr>
              <td><%= dat.pos %></td>
              <td><%= dat.nom %></td>
              <td><%= dat.assignat %></td>
              <td><%= dat.estat %></td>
              <td><%= dat.comentaris %></td>
              <%  }); %>
            </tr>
          </table>
          <button type="button" onclick="tanca()">Tancar</button>
        </section>
      </main>
      <!-- FOOTER -->
    <%- include("partials/paginacio") %>
</body>
</html>